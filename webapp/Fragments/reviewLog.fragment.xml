						<Label text="No." design="Bold"/>
					</Column>
					<Column hAlign="Center">
						<Label text="Date" design="Bold"/>
					</Column>
					<Column hAlign="Center">
						<Label text="Time" design="Bold"/>
					</Column>
					<Column hAlign="Center">
						<Label text="Channel" design="Bold"/>
					</Column>
					<Column hAlign="Center">
						<Label text="IN/OUT" design="Bold"/>
					</Column>
					<Column hAlign="Center">
						<Label text="Document" design="Bold"/>
					</Column>
					<Column hAlign="Center">
						<Label text="" design="Bold"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem>
						<cells>
